<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.practice.mapper.UserMapper">

    <select id="selectUserByEmail" parameterType="String" resultType="UserVO">
    SELECT *
      FROM USERS
     WHERE EMAIL = #{value}
    </select>

    <select id="selectUserByUserId" parameterType="String" resultType="UserVO">
    SELECT *
      FROM USERS
     WHERE USER_ID = #{value}
    </select>
    
</mapper>
